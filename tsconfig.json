{
	"extends": [
		"@tsconfig/strictest/tsconfig.json",
		"@tsconfig/esm/tsconfig.json"
	],
	"include": ["src", "README.ts"],
	"compilerOptions": {
		"noEmit": true,
		"skipLibCheck": false,
		"target": "esnext",
